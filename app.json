{
  "expo": {
    "name": "Spend Tracker",
    "slug": "spend_tracker",
    "scheme": "spend_tracker",
    "version": "1.2.0",
    "newArchEnabled": true,
    "orientation": "portrait",
    "icon": "./assets/and_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/landing.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/landing.png",
        "backgroundColor": "#6987ff"
      },
      "package": "com.waimin.incomeexpense"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8c52208a-eeed-450e-b8cd-c36c066650f5"
      }
    }
  }
}
